{
  "name": "blockwallet",
  "version": "0.0.0",
  "private": true,
  "babel": {
    "presets": [
      "env",
      "es2015",
      "stage-0"
    ]
  },
  "scripts": {
    "start": "nodemon bin/www --exec babel-node --presets es2015",
    "start-babel": "babel-node bin/www --presets es2015",
    "debug": "babel-node --inspect ./bin/www --exec babel-node --presets es2015",
    "test": "mocha --require babel-polyfill --compilers js:babel-register",
    "start-pm2": "pm2 start --interpreter babel-node bin/www --watch --ignore-watch='node_modules' --log-date-format='YYYY-MM-DD HH:mm Z' -f",
    "log-pm2": "pm2 logs --log-date-format='YYYY-MM-DD HH:mm Z' --lines 1000",
    "kill-pm2": "pm2 kill"
  },
  "dependencies": {
    "artoo-js": "^0.4.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "bcrypt": "^3.0.6",
    "body-parser": "~1.17.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "debug": "~2.6.3",
    "elasticsearch": "^14.0.0",
    "express": "~4.15.2",
    "express-jwt": "^5.3.0",
    "express-validator": "^4.3.0",
    "helmet": "^3.18.0",
    "jade": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "morgan": "~1.8.1",
    "node-schedule": "^1.3.2",
    "request": "^2.83.0",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {}
}
